//return largest number in the array

function returnLargest(a){
    arr = a;  
    max = 0;
    newArr = [];
    for(i = 0; i < arr.length; i++){
        if(arr[i] > max){
            max = arr[i];
        }
    }
    newArr.push(max);
    return newArr
}